prototype(Dotpulse.Base:InlineScript) < prototype(TYPO3.TypoScript:Tag) {
	filename = false
	@context.filename = ${this.filename}
	tagName = 'script'
	attributes {
		type = 'text/javascript'
	}
	content = TYPO3.TypoScript:Template {
		templatePath = ${'resource://' + sitePackage + '/Private/Templates/InlineAssets/' + filename + '.js'}
	}
	@if.hasFilename = ${this.filename}
}
